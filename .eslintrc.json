{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true,
        "node": true
        },
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
        },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
        },
    "ignorePatterns": ["dist", "coverage"],
    "rules": {
        "use-isnan": "off"
        }
    }